<h1> Update your dog's information</h1>
<%= form_for @dog do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br/>
  <%= f.label :age %>
  <%= f.email_field :age %><br/>
  <%= f.label :size %>; small, medium, or large:
  <%= f.number_field :size %><br/>
  <%= f.label :vet_name %>
  <%= f.text_field :vet_name %><br/>
  <%= f.label :food_name %>
  <%= f.text_field :food_name %><br/>
  <%= f.label :picture %>;  picture link:
  <%= f.url_field :img_url %><br/>
  <%= f.submit %>
<% end %>
